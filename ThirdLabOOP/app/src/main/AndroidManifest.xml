<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.thirdlaboop">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_baseline_casino_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_casino_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.ThirdLabOOP"
        tools:targetApi="31">
        <activity
            android:name=".FourthActivity"
            android:exported="false" />
        <activity
            android:name=".FinalPage"
            android:exported="false"
            android:launchMode="singleInstance" />
        <activity
            android:name=".ThirdPage"
            android:exported="false"
            android:launchMode="singleInstance" />
        <activity
            android:name=".SecondPage"
            android:exported="false"
            android:launchMode="singleInstance" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>